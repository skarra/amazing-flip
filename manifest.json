{
    "name": "Amazon lookup for Flipkart",
    "version": "1.0",
    "manifest_version": 2,

    "description": "Open the Amazon page for the book that is being view on Flipkart at the moment",

    "background" : {
	"scripts" : ["amazing-flip.js"]
    },

    "icons" : {
	"16" : "amazing-flip-16.png",
	"48" : "amazing-flip-48.png",
	"128" : "amazing-flip-128.png"
    },

    "permissions" : ["tabs", "contextMenus"]
}
